{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/proxy_image.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/proxy_image",
      "dest": "api/proxy_image.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
